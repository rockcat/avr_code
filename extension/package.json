{
    "name": "avr-support",
    "displayName": "AVR Support",
    "description": "AVR Assembly language support",
    "version": "0.0.2",
    "publisher": "rockcat",
    "engines": {
        "vscode": "^1.5.0"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/rockcat/avr_code.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
    ],
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "avr",
                "aliases": [
                    "AVR Assembler"
                ],
                "extensions": [
                    ".asm", ".inc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [{
            "language": "avr",
            "scopeName": "source.avrasm",
            "path": "./syntaxes/avrasm.tmLanguage"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
